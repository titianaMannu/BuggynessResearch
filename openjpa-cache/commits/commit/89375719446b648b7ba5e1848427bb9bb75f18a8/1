+        em.getTransaction().commit();\n+        em.close();\n+    }\n+    \n+    public void testReloadTrue() {\n+        compat.setReloadOnDetach(true);\n+        detachProcessing();\n+    }\n+    \n+    public void testReloadFalse() {\n+        compat.setReloadOnDetach(false);\n+        detachProcessing();\n+    }\n+    \n+    private void detachProcessing() {\n+        // Detach individual entities explicitly\n+        em = emf.createEntityManager();\n+        em.getTransaction().begin();\n+        intVer = em.find(IntVersionEntity.class, id);\n+        tsVer = em.find(TimestampVersionEntity.class, id);\n+        noVer = em.find(NoVersionEntity.class, id);\n+        em.detach(intVer);\n+        em.detach(tsVer);\n+        em.detach(noVer);\n+        em.getTransaction().commit();\n+        em.close();\n+        \n+        // Detach all internal implicitly with close()\n+        em = emf.createEntityManager();\n+        em.getTransaction().begin();\n+        intVer = em.find(IntVersionEntity.class, id);\n+        tsVer = em.find(TimestampVersionEntity.class, id);\n+        noVer = em.find(NoVersionEntity.class, id);\n+        em.getTransaction().commit();\n+        em.close();\n+        \n+    }\n+}"},{"sha":"284354ff006b18560daae2e54e9c95ae371be218","filename":"openjpa-persistence-jdbc/src/test/java/org/apache/openjpa/persistence/detach/TimestampVersionEntity.java","status":"added","additions":63,"deletions":0,"changes":63,"blob_url":"https://github.com/apache/openjpa/blob/89375719446b648b7ba5e1848427bb9bb75f18a8/openjpa-persistence-jdbc/src/test/java/org/apache/openjpa/persistence/detach/TimestampVersionEntity.java","raw_url":"https://github.com/apache/openjpa/raw/89375719446b648b7ba5e1848427bb9bb75f18a8/openjpa-persistence-jdbc/src/test/java/org/apache/openjpa/persistence/detach/TimestampVersionEntity.java","contents_url":"https://api.github.com/repos/apache/openjpa/contents/openjpa-persistence-jdbc/src/test/java/org/apache/openjpa/persistence/detach/TimestampV